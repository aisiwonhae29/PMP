var http = require('http');
var url = require('url');

var server = http.createServer((req, res) => {
    if(req.url == '/now'){
        res.writeHead(200 , {'ct' : 'a/j'});
        res.write(JSON.stringify({now:Date(),'test' : req.url} ));
        res.write('hihi');
        res.end();
    }else{
        res.write('invalid url');
        res.end();
    }
}).listen(3000);
console.log('server running on port')